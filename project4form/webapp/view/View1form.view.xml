<mvc:View 
    controllerName="project4form.controller.View1form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    displayBlock="true">
    <Page id="page" title="{i18n>title}">
        <content>
           
                <GenericTile class="sapUiMediumMargin"
                    id="_IDGenButton"
                    press=".onMaterialFormPress"
                    header="Material Form">
                    <TileContent id="_IDGenTileContent1">
                        <VBox id="_IDGenVBox" justifyContent="Center" alignItems="Center" >
                            <core:Icon id="_IDGenIcon" src="sap-icon://product" size="3rem" />
                        </VBox>
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiMediumMargin"
                    id="_IDGenButton1"
                    press=".onServiceFormPress"
                    header="Service Form">
                    <TileContent id="_IDGenTileContent2">
                        <VBox id="_IDGenVBox1" justifyContent="Center" alignItems="Center">
                            <core:Icon id="_IDGenIcon1" src="sap-icon://enablement" size="3rem" />
                        </VBox>
                    </TileContent>
                </GenericTile>

        </content>
    </Page>
</mvc:View>
