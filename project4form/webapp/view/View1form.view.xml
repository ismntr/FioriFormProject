<mvc:View 
    controllerName="project4form.controller.View1form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    displayBlock="true">
    <Page id="page" title="{i18n>title}">
        <content>
        
                <GenericTile class="sapUiMediumMargin"
                    id="_IDGenButton"
                    press=".onMaterialFormPress"
                    header="Material Form" backgroundImage="\test\2.jpg" frameType="TwoByOne">
                    <TileContent id="_IDGenTileContent1" footer="LST.EYS.FRM.71" >
                        <VBox id="_IDGenVBox"  alignItems="Start"  >
                            <core:Icon id="_IDGenIcon" src="sap-icon://product" size="3rem" />
                        </VBox>
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiMediumMargin"
                    id="_IDGenButton1"
                    press=".onServiceFormPress"
                    header="Service Form" backgroundImage="\test\1.jpg" frameType="TwoByOne" >
                    <TileContent id="_IDGenTileContent2" footer="LST.EYS.FRM.116" >
                        <VBox id="_IDGenVBox1"  alignItems="Start">
                            <core:Icon id="_IDGenIcon1" src="sap-icon://people-connected" size="3rem" />
                        </VBox>
                    </TileContent>
                </GenericTile>

        </content>
    </Page>
</mvc:View>
